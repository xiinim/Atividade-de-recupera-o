package projetoInicial;

import java.util.Scanner;


class subtracao {
	public static int add(int n1, int n2){
		return n1 - n2;
	}
}
	
class adicao {
	public static int add(int n1, int n2){
		return n1 + n2;
	}
}
		
class multiplicacao {
	public static int add(int n1, int n2){
		return n1 * n2;
	}
}
		
class divisao {
	public static int add(int n1, int n2){
		return n1 / n2;
	}
}

